<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "https://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="AD">
    <class name="AD.Provincia" table="PROVINCIA">
        <id name="IdProvincia" column="ID_PROVINCIA">
            <generator class="native"/>
        </id>   
        <property name="Nombre" column="NOMBRE"/>
        <property name="CodProvincia" column="COD_PROVINCIA"/>
    </class>
</hibernate-mapping>